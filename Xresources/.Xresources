! Base16 Atelier Lakeside
! Scheme: Bram de Haan (http://atelierbram.github.io/syntax-highlighting/atelier-schemes/lakeside/)

#define base00 #161b1d
#define base01 #1f292e
#define base02 #516d7b
#define base03 #5a7b8c
#define base04 #7195a8
#define base05 #7ea2b4
#define base06 #c1e4f6
#define base07 #ebf8ff
#define base08 #d22d72
#define base09 #935c25
#define base0A #8a8a0f
#define base0B #568c3b
#define base0C #2d8f6f
#define base0D #257fad
#define base0E #6b6bb8
#define base0F #b72dd2

! human-friendly colors
# define white #ffffff
!!! grayscale
# define gray0  base00
# define gray1  base01
# define gray2  base02
# define gray3  base03
# define gray4  base04
# define gray5  base05
# define gray6  base06
# define gray7  base07
!!! extra colors
#define yellow  base0A
#define orange  base09
#define red     base08
#define magenta base0F
#define violet  base0E
#define blue    base0D
#define cyan    base0C
#define green   base0B

*.foreground:   base05
*.background:   base00
*.cursorColor:  base05

*.color0:       base00
*.color1:       base08
*.color2:       base0B
*.color3:       base0A
*.color4:       base0D
*.color5:       base0E
*.color6:       base0C
*.color7:       base05

*.color8:       base03
*.color9:       base09
*.color10:      base0C
*.color11:      base02
*.color12:      base04
*.color13:      base06
*.color14:      base0F
*.color15:      base07

URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
URxvt.scrollstyle: rxvt
URxvt.perl-ext-common: default,matcher,resize-font
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! rofi.color-normal: gray1,blue,gray1,blue,white
! activebg, text, pinstripe1, potentialprog bg, potentialprog fg
rofi.color-normal: gray0,blue,gray1,blue,white
rofi.color-window: gray0, gray0
rofi.color-active: gray0, gray1
